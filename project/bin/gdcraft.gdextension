[configuration]

entry_symbol = "gdcraft_library_init"
compatibility_minimum = "4.5"
reloadable = true

[libraries]

macos.debug = "./libgdcraft-extension.macos.template_debug.dylib"
macos.release = "./libgdcraft-extension.macos.template_release.dylib"
windows.debug.x86_32 = "./gdcraft-extension.windows.template_debug.x86_32.dll"
windows.release.x86_32 = "./gdcraft-extension.windows.template_release.x86_32.dll"
windows.debug.x86_64 = "./gdcraft-extension.windows.template_debug.x86_64.dll"
windows.release.x86_64 = "./gdcraft-extension.windows.template_release.x86_64.dll"
linux.debug.x86_64 = "./libgdcraft-extension.linux.template_debug.x86_64.so"
linux.release.x86_64 = "./libgdcraft-extension.linux.template_release.x86_64.so"
linux.debug.arm64 = "./libgdcraft-extension.linux.template_debug.arm64.so"
linux.release.arm64 = "./libgdcraft-extension.linux.template_release.arm64.so"
linux.debug.rv64 = "./libgdcraft-extension.linux.template_debug.rv64.so"
linux.release.rv64 = "./libgdcraft-extension.linux.template_release.rv64.so"